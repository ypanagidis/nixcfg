[
  {
    "key": "shift+k",
    "command": "editor.action.showHover",
    "when": "editorTextFocus && vim.mode === 'Normal'"
  },
  {
    "key": "j",
    "command": "editor.action.scrollDownHover",
    "when": "editorHoverVisible"
  },
  {
    "key": "k",
    "command": "editor.action.scrollUpHover",
    "when": "editorHoverVisible"
  },
  {
    "key": "l",
    "command": "editor.action.scrollRightHover",
    "when": "editorHoverVisible"
  },
  {
    "key": "h",
    "command": "editor.action.scrollLeftHover",
    "when": "editorHoverVisible"
  },
  {
    "key": "ctrl+u",
    "command": "editor.action.pageUpHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.pageDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "q",
    "command": "extension.vim_escape",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl+c",
    "command": "extension.vim_escape",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl+c",
    "command": "workbench.action.closeQuickOpen",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.view.explorer",
    "when": "vim.mode === 'Normal' && editorTextFocus"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "vim.mode === 'Normal' && explorerViewletVisible"
  },
  {
    "key": "i",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "j",
    "command": "renameFile",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "y",
    "command": "filesExplorer.copy",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "x",
    "command": "filesExplorer.cut",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "p",
    "command": "filesExplorer.paste",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "d",
    "command": "deleteFile",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "j",
    "command": "list.focusDown",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "key": "k",
    "command": "list.focusUp",
    "when": "filesExplorerFocus && !inputFocus"
  },
  { "key": "ctrl+j", "command": "quickInput.next", "when": "inQuickOpen" },
  { "key": "ctrl+k", "command": "quickInput.previous", "when": "inQuickOpen" },
  { "key": "ctrl+l", "command": "workbench.action.focusNextGroup" },
  { "key": "ctrl+h", "command": "workbench.action.focusPreviousGroup" },
  {
    "key": "shift+l",
    "command": "workbench.action.nextEditorInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "shift+h",
    "command": "workbench.action.previousEditorInGroup",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.focus",
    "when": "vim.mode === 'Normal' && !inQuickOpen"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+a m",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+a m",
    "command": "workbench.action.closePanel",
    "when": "editorTextFocus"
  },

  { "key": "ctrl+i", "command": "composerMode.agent" },

  {
    "key": "ctrl+q",
    "command": "workbench.action.closeQuickOpen",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+q",
    "command": "hideCodeActionWidget",
    "when": "codeActionMenuVisible || actionWidgetVisible"
  },
  {
    "key": "ctrl+k",
    "command": "hideCodeActionWidget",
    "when": "codeActionMenuVisible || actionWidgetVisible"
  },
  {
    "key": "q",
    "command": "closeReferenceSearch",
    "when": "referenceSearchVisible"
  },
  { "key": "q", "command": "editor.closePeek", "when": "peekWidgetVisible" },
  {
    "key": "q",
    "command": "hideSuggestWidget",
    "when": "suggestWidgetVisible"
  },
  {
    "key": "q",
    "command": "closeParameterHints",
    "when": "parameterHintsVisible"
  },
  {
    "key": "q",
    "command": "editor.action.inlineSuggest.hide",
    "when": "inlineSuggestionVisible"
  },
  { "key": "q", "command": "closeFindWidget", "when": "findWidgetVisible" },
  {
    "key": "q",
    "command": "editor.action.closeHover",
    "when": "editorHoverVisible"
  },
  { "key": "q", "command": "cancelRenameInput", "when": "renameInputVisible" },
  {
    "key": "q",
    "command": "workbench.action.closeMessages",
    "when": "notificationCenterVisible || notificationToastsVisible"
  },
  {
    "key": "q",
    "command": "workbench.action.closePanel",
    "when": "panelFocus || (panelVisible && !editorFocus)"
  }
  //   {
  //     "key": "ctrl+w m",
  //     "command": "workbench.action.toggleMaximizedPanel",
  //     "when": "!panelMaximized"
  //   },
  //   {
  //     "key": "ctrl+w m",
  //     "command": "workbench.action.togglePanel",
  //     "when": "panelVisible"
  //   }
]
